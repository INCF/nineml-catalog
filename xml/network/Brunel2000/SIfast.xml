<?xml version='1.0' encoding='UTF-8'?>
<NineML xmlns="http://nineml.net/9ML/2.0">
  <Unit symbol="Hz" dimension="per_time" power="0"/>
  <Selection name="All">
    <Concatenate>
      <Item index="0">
        <Reference name="Exc"/>
      </Item>
      <Item index="1">
        <Reference name="Inh"/>
      </Item>
    </Concatenate>
  </Selection>
  <Dimension name="per_time" t="-1"/>
  <Population name="Exc">
    <Size>10000</Size>
    <Cell>
      <DynamicsProperties name="nrn">
        <Definition name="LeakyIntegrateAndFire" url="../../neuron/LeakyIntegrateAndFire.xml"/>
        <Property units="Mohm" name="R">
          <SingleValue>1.5</SingleValue>
        </Property>
        <Property units="ms" name="refractory_period">
          <SingleValue>2.0</SingleValue>
        </Property>
        <Property units="ms" name="tau">
          <SingleValue>20.0</SingleValue>
        </Property>
        <Property units="mV" name="v_reset">
          <SingleValue>10.0</SingleValue>
        </Property>
        <Property units="mV" name="v_threshold">
          <SingleValue>20.0</SingleValue>
        </Property>
        <Initial units="ms" name="refractory_end">
          <SingleValue>0.0</SingleValue>
        </Initial>
        <Initial units="mV" name="v">
          <RandomDistributionProperties name="uniform_rest_to_threshold">
            <Definition name="UniformDistribution" url="../../randomdistribution/Uniform.xml"/>
            <Property units="unitless" name="maximum">
              <SingleValue>20.0</SingleValue>
            </Property>
            <Property units="unitless" name="minimum">
              <SingleValue>0.0</SingleValue>
            </Property>
          </RandomDistributionProperties>
        </Initial>
      </DynamicsProperties>
    </Cell>
  </Population>
  <Projection name="Excitation">
    <Source>
      <Reference name="Exc"/>
    </Source>
    <Destination>
      <Reference name="All"/>
      <FromResponse send_port="i_synaptic" receive_port="i_synaptic"/>
    </Destination>
    <Connectivity>
      <ConnectionRuleProperties name="RandomExc">
        <Definition name="RandomFanIn" url="../../connectionrule/RandomFanIn.xml"/>
        <Property units="unitless" name="number">
          <SingleValue>1000</SingleValue>
        </Property>
      </ConnectionRuleProperties>
    </Connectivity>
    <Response>
      <DynamicsProperties name="syn">
        <Definition name="Alpha" url="../../postsynapticresponse/Alpha.xml"/>
        <Property units="ms" name="tau">
          <SingleValue>0.1</SingleValue>
        </Property>
        <Initial units="nA" name="a">
          <SingleValue>0.0</SingleValue>
        </Initial>
        <Initial units="nA" name="b">
          <SingleValue>0.0</SingleValue>
        </Initial>
      </DynamicsProperties>
      <FromPlasticity send_port="fixed_weight" receive_port="weight"/>
    </Response>
    <Plasticity>
      <DynamicsProperties name="ExcitatoryPlasticity">
        <Definition name="Static" url="../../plasticity/Static.xml"/>
        <Property units="nA" name="weight">
          <SingleValue>13.7707633471</SingleValue>
        </Property>
      </DynamicsProperties>
    </Plasticity>
    <Delay units="ms">
      <SingleValue>1.5</SingleValue>
    </Delay>
  </Projection>
  <Unit symbol="nA" dimension="current" power="-9"/>
  <Unit symbol="mV" dimension="voltage" power="-3"/>
  <Dimension i="-1" l="2" m="1" t="-3" name="voltage"/>
  <Dimension i="-2" l="2" m="1" t="-3" name="resistance"/>
  <Dimension i="1" name="current"/>
  <Population name="Ext">
    <Size>12500</Size>
    <Cell>
      <DynamicsProperties name="stim">
        <Definition name="Poisson" url="../../input/Poisson.xml"/>
        <Property units="Hz" name="rate">
          <SingleValue>38729.3950153</SingleValue>
        </Property>
        <Initial units="ms" name="t_next">
          <RandomDistributionProperties name="exponential_beta">
            <Definition name="ExponentialDistribution" url="../../randomdistribution/Exponential.xml"/>
            <Property units="unitless" name="rate">
              <SingleValue>38729.3950153</SingleValue>
            </Property>
          </RandomDistributionProperties>
        </Initial>
      </DynamicsProperties>
    </Cell>
  </Population>
  <Projection name="Inhibition">
    <Source>
      <Reference name="Inh"/>
    </Source>
    <Destination>
      <Reference name="All"/>
      <FromResponse send_port="i_synaptic" receive_port="i_synaptic"/>
    </Destination>
    <Connectivity>
      <ConnectionRuleProperties name="RandomInh">
        <Definition name="RandomFanIn" url="../../connectionrule/RandomFanIn.xml"/>
        <Property units="unitless" name="number">
          <SingleValue>250</SingleValue>
        </Property>
      </ConnectionRuleProperties>
    </Connectivity>
    <Response>
      <DynamicsProperties name="syn">
        <Definition name="Alpha" url="../../postsynapticresponse/Alpha.xml"/>
        <Property units="ms" name="tau">
          <SingleValue>0.1</SingleValue>
        </Property>
        <Initial units="nA" name="a">
          <SingleValue>0.0</SingleValue>
        </Initial>
        <Initial units="nA" name="b">
          <SingleValue>0.0</SingleValue>
        </Initial>
      </DynamicsProperties>
      <FromPlasticity send_port="fixed_weight" receive_port="weight"/>
    </Response>
    <Plasticity>
      <DynamicsProperties name="InhibitoryPlasticity">
        <Definition name="Static" url="../../plasticity/Static.xml"/>
        <Property units="nA" name="weight">
          <SingleValue>-82.6245800828</SingleValue>
        </Property>
      </DynamicsProperties>
    </Plasticity>
    <Delay units="ms">
      <SingleValue>1.5</SingleValue>
    </Delay>
  </Projection>
  <Population name="Inh">
    <Size>2500</Size>
    <Cell>
      <DynamicsProperties name="nrn">
        <Definition name="LeakyIntegrateAndFire" url="../../neuron/LeakyIntegrateAndFire.xml"/>
        <Property units="Mohm" name="R">
          <SingleValue>1.5</SingleValue>
        </Property>
        <Property units="ms" name="refractory_period">
          <SingleValue>2.0</SingleValue>
        </Property>
        <Property units="ms" name="tau">
          <SingleValue>20.0</SingleValue>
        </Property>
        <Property units="mV" name="v_reset">
          <SingleValue>10.0</SingleValue>
        </Property>
        <Property units="mV" name="v_threshold">
          <SingleValue>20.0</SingleValue>
        </Property>
        <Initial units="ms" name="refractory_end">
          <SingleValue>0.0</SingleValue>
        </Initial>
        <Initial units="mV" name="v">
          <RandomDistributionProperties name="uniform_rest_to_threshold">
            <Definition name="UniformDistribution" url="../../randomdistribution/Uniform.xml"/>
            <Property units="unitless" name="maximum">
              <SingleValue>20.0</SingleValue>
            </Property>
            <Property units="unitless" name="minimum">
              <SingleValue>0.0</SingleValue>
            </Property>
          </RandomDistributionProperties>
        </Initial>
      </DynamicsProperties>
    </Cell>
  </Population>
  <Projection name="External">
    <Source>
      <Reference name="Ext"/>
    </Source>
    <Destination>
      <Reference name="All"/>
      <FromResponse send_port="i_synaptic" receive_port="i_synaptic"/>
    </Destination>
    <Connectivity>
      <ConnectionRuleProperties name="OneToOne">
        <Definition name="OneToOne" url="../../connectionrule/OneToOne.xml"/>
      </ConnectionRuleProperties>
    </Connectivity>
    <Response>
      <DynamicsProperties name="syn">
        <Definition name="Alpha" url="../../postsynapticresponse/Alpha.xml"/>
        <Property units="ms" name="tau">
          <SingleValue>0.1</SingleValue>
        </Property>
        <Initial units="nA" name="a">
          <SingleValue>0.0</SingleValue>
        </Initial>
        <Initial units="nA" name="b">
          <SingleValue>0.0</SingleValue>
        </Initial>
      </DynamicsProperties>
      <FromPlasticity send_port="fixed_weight" receive_port="weight"/>
    </Response>
    <Plasticity>
      <DynamicsProperties name="ExternalPlasticity">
        <Definition name="Static" url="../../plasticity/Static.xml"/>
        <Property units="nA" name="weight">
          <SingleValue>13.7707633471</SingleValue>
        </Property>
      </DynamicsProperties>
    </Plasticity>
    <Delay units="ms">
      <SingleValue>1.5</SingleValue>
    </Delay>
  </Projection>
  <Unit symbol="ms" dimension="time" power="-3"/>
  <Dimension name="time" t="1"/>
  <Unit symbol="Mohm" dimension="resistance" power="6"/>
  <Dimension name="dimensionless"/>
  <Unit symbol="unitless" dimension="dimensionless" power="0"/>
</NineML>
